<!DOCTYPE web-app PUBLIC  "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"  "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
  <display-name>Archetype Created Web Application</display-name>
  

<servlet>
  	<servlet-name>Login</servlet-name>
  	<display-name>Login</display-name>
  	<description></description>
  	<servlet-class>restservice.Login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SendToAllUser</servlet-name>
  	<display-name>SendToAllUser</display-name>
  	<description></description>
  	<servlet-class>restservice.SendToAllUser</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FetchSpecificUser</servlet-name>
  	<display-name>FetchSpecificUser</display-name>
  	<description></description>
  	<servlet-class>restservice.FetchSpecificUser</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddBotChat</servlet-name>
  	<display-name>AddBotChat</display-name>
  	<description></description>
  	<servlet-class>restservice.AddBotChat</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SendChatMessage</servlet-name>
  	<display-name>SendChatMessage</display-name>
  	<description></description>
  	<servlet-class>restservice.SendChatMessage</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SendToAllUser</servlet-name>
  	<url-pattern>/sendtoalluser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FetchSpecificUser</servlet-name>
  	<url-pattern>/fetchspecificuser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddBotChat</servlet-name>
  	<url-pattern>/addbotchat</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SendChatMessage</servlet-name>
  	<url-pattern>/sendchatmessage</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>restservice.AppContextListener</listener-class>
</listener>
</web-app>
